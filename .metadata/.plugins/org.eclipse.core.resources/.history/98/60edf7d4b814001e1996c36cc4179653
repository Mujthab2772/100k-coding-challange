#include <stdio.h>
#include <stdlib.h>

int main(void) {
	char a,b,c,d,e,f,g;
	printf("Enter your name.");
	scanf("%c%c%c%c%c%c%c",&a,&b,&c,&d,&e,&f,&g);
	printf("You entered %c",a,b,c,d,e,f,g);
	return EXIT_SUCCESS;
}
