#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float totalmark;
	printf("Enter your mark here?");
	scanf("%f",&totalmark);
	if(&totalmark>90 &totalmark<100){
		printf("A grade");
	}
	return EXIT_SUCCESS;
}
